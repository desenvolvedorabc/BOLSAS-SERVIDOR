{
  "name": "parc-bolsas-backend",
  "version": "0.0.1",
  "description": "",
  "author": "Going",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "documentation": "npx @compodoc/compodoc -p tsconfig.json -s",
    "typedoc": "npx typedoc --tsconfig tsconfig.json --entryPointStrategy expand",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config -n dist/src/database/typeorm/config-migrations.js",
    "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed -n dist/src/database/typeorm/config-migrations.js",
    "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config src/database/typeorm/ormconfig.ts",
    "db:develop:migration:generate": "npx dotenv -e .env.develop -- yarn typeorm:cli migration:generate -n",
    "db:staging:migration:generate": "npx dotenv -e .env.staging -- yarn typeorm:cli migration:generate -n"
  },
  "dependencies": {
    "@microsoft/tsdoc": "^0.13.2",
    "@nestjs/common": "^7.6.15",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.15",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/mapped-types": "^1.1.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.6.15",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^4.8.0",
    "@nestjs/throttler": "^3.1.0",
    "@nestjs/typeorm": "^7.1.5",
    "@nestjsx/crud": "^5.0.0-alpha.3",
    "@nestjsx/crud-typeorm": "^5.0.0-alpha.3",
    "@types/body-parser": "^1.19.2",
    "@types/date-fns": "^2.6.0",
    "@types/multer": "^1.4.7",
    "@types/nodemailer": "^6.4.4",
    "@types/uuid": "^8.3.4",
    "body-parser": "^1.20.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.1",
    "date-fns": "^2.29.3",
    "exceljs": "^4.3.0",
    "module-alias": "^2.2.2",
    "multer": "^1.4.4",
    "mysql2": "^2.3.3",
    "nestjs-pino": "^2.5.0",
    "nestjs-typeorm-paginate": "^3.1.3",
    "nodemailer": "^6.7.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pino": "^7.6.4",
    "pino-http": "^6.6.0",
    "pino-pretty": "^9.0.1",
    "postinstall": "^0.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "0.2.32",
    "typeorm-encrypted": "^0.5.6",
    "typeorm-seeding": "^1.6.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.18",
    "@faker-js/faker": "^6.3.1",
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "^7.3.0",
    "@nestjs/testing": "^7.6.15",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.36",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "dotenv-cli": "^7.2.1",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-tsdoc": "^0.2.14",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "source-map-support": "0.5.21",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.0.18",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.9.0",
    "typedoc": "^0.22.10",
    "typedoc-theme-hierarchy": "^1.0.14",
    "typescript": "^4.2.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
